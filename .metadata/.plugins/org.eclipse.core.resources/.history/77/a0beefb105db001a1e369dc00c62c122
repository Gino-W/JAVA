package C_14_URL;

import java.io.IOException;
import java.net.URL;

/**
 * @author: Gino
 * @Data: 10 Aug 202020:29:58
 * @File_Name: URLDemo.java
 * 
 */
public class URLDemo {
	public static void main(String[] args) throws IOException {
		URL url = new URL("127.0.0.1:8080");
		url.getProtocol();
		url.getFile();
		url.getPort();
		url.getQuery();
		
	}

}
