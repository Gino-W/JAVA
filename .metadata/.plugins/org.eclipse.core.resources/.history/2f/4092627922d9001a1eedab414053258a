 */
public class String_Example_1 {
	public static void main(String[] args) {
//			String str1 = "a";
//			String str2 = "b";
//			System.out.println(str1.compareTo(str2));
		
		String[] str = {"abc","def","opq","jkl"};
		printArr(str);
	}
	
	
	//打印数组
	public static void printArr(String[] s) {
		for(String st : s) {
			System.out.println(st);
		}
	} 

}
